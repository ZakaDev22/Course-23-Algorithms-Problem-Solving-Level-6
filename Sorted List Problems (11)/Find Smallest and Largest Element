

SortedSet<int> sortedSet = new SortedSet<int> { 1, 2, 3, 4, 5 };

var tupleResult = RetirnMaxAndMinInList(sortedSet);

Console.WriteLine($"\n\n Sorted Set Elements : {string.Join(", ", sortedSet)}");
Console.WriteLine($"\n\n Min Number = {tupleResult.Min}, Max Number = {tupleResult.Max}");

(int Min, int Max) RetirnMaxAndMinInList(SortedSet<int> sortedSet)
{
    int min = 0, max = 0;

    min = sortedSet.Min();
    max = sortedSet.Max();

    return (min, max);
}

