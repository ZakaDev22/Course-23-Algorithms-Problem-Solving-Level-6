
var binaryTree = new BinaryTree<int>();

binaryTree.Insert(5);
binaryTree.Insert(3);
binaryTree.Insert(8);
binaryTree.Insert(1);
binaryTree.Insert(10);
binaryTree.Insert(6);
binaryTree.Insert(9);
binaryTree.Insert(15);

var binaryTree2 = new BinaryTree<int>();

binaryTree2.Insert(5);
binaryTree2.Insert(3);
binaryTree2.Insert(8);
binaryTree2.Insert(1);
binaryTree2.Insert(10);
binaryTree2.Insert(6);
binaryTree2.Insert(9);
binaryTree2.Insert(15);

Console.WriteLine($"\n\n Are The Two Trees Identical ? ");

if (binaryTree.MaxDepth(binaryTree.Root) == binaryTree2.MaxDepth(binaryTree2.Root))
{
    Console.WriteLine($"\n Yes The Trees Are Identical");
}
else
    Console.WriteLine($"\n No The Trees Are Not Identical");
